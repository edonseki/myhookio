{
    "manifest_version": 2,
    "name": "MyHook",
    "description": "MyHook allows you to expose your local webserver to public. It's very helpful to test API, Webhooks and other services.",
    "version": "1.0",

    "icons": {
        "48": "image.png"
    },

    "background": {
        "scripts" : ["js/jquery.js", "js/socket.io.js", "js/logic.js","js/background.js"]
    },
    
    "browser_action": {
        "default_icon": "image.png",
        "default_title": "MyHook"
    },

    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}